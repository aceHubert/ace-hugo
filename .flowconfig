[ignore]
.*/node_modules/.*
./build/.*
./test/.*
./dist/.*

[include]
.*/node_modules/codemirror/.*

[libs]
flow-typed

[options]
esproposal.class_static_fields=enable
module.ignore_non_literal_requires=true
module.file_ext=.js
module.file_ext=.jsx
module.file_ext=.json
module.file_ext=.css
module.file_ext=.scss
module.name_mapper='.*\(.s?css\)' -> 'css-module-flow'
